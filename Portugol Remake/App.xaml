<Application
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Portugol_Remake"
             xmlns:System="clr-namespace:System;assembly=mscorlib" x:Class="Portugol_Remake.App"
             Startup="Application_Startup">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary x:Name="brighttheme" Source="Resources/ResourceDictionaryBrightTheme.xaml"/>
                <ResourceDictionary x:Name="darktheme" Source="Resources/ResourceDictionaryDarkTheme.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
